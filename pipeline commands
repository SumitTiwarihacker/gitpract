pipeline{
    agent any
    stages{
        stage("Git Clone"){
            steps{
                git "https://github.com/hellokaton/java11-examples.git"
            }
        }
        stage("Build using maven"){
            steps{
                sh "mvn clean package"
            }
        }
        stage("Archive"){
            steps{
                archiveArtifacts artifacts: 'target/*.jar'
                echo "Done"
            }
        }
    }
}
